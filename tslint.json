{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": false,
        "eofline": false,
        "array-type": false,
        "object-literal-sort-keys": false,
        "object-literal-shorthand": false,
        "one-line": false,
        "trailing-comma": false,
        "semicolon": true
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "tst/**"
        ]
    }
}